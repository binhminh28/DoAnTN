                        <form action="/checkout1" method="POST" enctype="application/x-www-form-urlencoded">
                            <h3 class="title">
                                Shopping Cart
                            </h3>
                            <div class="clearfix">
                            </div>
                            <table class="shop-table">
                                <tbody>
                                    <h4 style="text-align:center; color:red" ng-show="!(products| filter:nameText).length">Your Shopping Cart is empty.</h4>

                                    <tr ng-repeat="x in products| filter:nameText">
                                        
                                        
                                        <td>
                                            <input  type="text" name="productID" placeholder="Product Name" value="{{x._id}}"> 
                                            <input  type="text" name="soluong" placeholder="Product Name" value="{{x.sl}}"> 
                                            <input  type="text" name="size" placeholder="Product Name" value="{{x.size}}"> 
                                        </td>
                                        <td>
                                            <img src="{{x.image}}" alt="">
                                        </td>
                                        <td>
                                            <div class="shop-details">
                                                <div class="productname">
                                                    {{x.name}}
                                                </div>
                                            </div>
                                        </td>
                                        <td >
                                            {{x.sl}}
                                        </td>
                                        <td>
                                            <h5>
                                                <strong class="red">
                                                    ${{x.price}}
                                                </strong>
                                            </h5>
                                        </td>
                                        <td>
                                            <a ng-click="removeItem($index)">
                                                <img src="images/remove.png" alt="">
                                            </a>
                                        </td>
                                    </tr>

                                </tbody>
                                <tfoot>
                                    <tr>
                                        <td colspan="6">
                                            <!-- <button class="pull-left">
                                            Continue Shopping
                                        </button>
                                        <button class=" pull-right">
                                            Update Shopping Cart
                                        </button> -->
                                        </td>
                                    </tr>
                                </tfoot>
                            </table>

